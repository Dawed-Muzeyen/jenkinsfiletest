pipeline {
  agent any
  stages {
   stage("Hello") {
     steps {
     echo "Hello world"
     }
   }
    stage("Build") {
     steps {
     echo "Building"
     }
   }
    stage("Test") {
     steps {
     echo "Testing"
     }
   }
    stage("Deploy") {
     steps {
     echo "Deploying"
     }
   }
    stage("Release") {
     steps {
     echo "Releasing"
     }
   }
  }
}
